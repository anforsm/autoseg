    "def is_valid(size):\n",
    "  if not size % 16 == 0:\n",
    "    return False\n",
    "  \n",
    "  depth = 3\n",
    "  for _ in range(depth):\n",
    "    size -= 2\n",
    "    if not size % 2 == 0:\n",
    "      return False\n",
    "    size /= 2\n",
    "  return True"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "metadata": {},
   "outputs": [],
   "source": [
    "for i in range(50000):\n",
    "  if is_valid(i):\n",
    "    print(i)"
   ]
